            {
              "name": "document_management",
              "level": 4,
              "description": "Manages account opening documents",
              "preferred_llm": {
                "provider": "anthropic",
                "model": "claude-3-sonnet-20240229",
                "reason": "Good for document creation and formatting"
              }
            }
          ],
          "llm_config": {
            "provider": "openai",
            "model": "gpt-4-turbo",
            "temperature": 0.2,
            "maxTokens": 4000
          },
          "workflows": ["account_opening_automation"],
          "system_prompt": {
            "role": "Account Opening Specialist",
            "goal": "Efficiently open customer accounts with complete documentation",
            "instructions": "Create instructions, scan documents, fill checklists, prepare final documents"
          }
        }
      },
      
      "notification_agent": {
        "name": "Notification Agent",
        "type": "communication",
        "description": "Multi-channel customer communication",
        "config": {
          "personality": {
            "friendliness": 0.9,
            "formality": 0.7,
            "proactiveness": 0.8,
            "detail_orientation": 0.8
          },
          "skills": [
            {
              "name": "multi_channel_communication",
              "level": 5,
              "description": "Communicates via email, phone, and mail",
              "preferred_llm": {
                "provider": "anthropic",
                "model": "claude-3-haiku-20240307",
                "reason": "Fast and good for communication tasks"
              },
              "capabilities": [
                {
                  "id": "email_notifications",
                  "name": "Email Notifications",
                  "description": "Sends automated email notifications",
                  "requiredTools": ["email_tool"],
                  "executionStrategy": "direct"
                },
                {
                  "id": "phone_notifications",
                  "name": "Phone Notifications",
                  "description": "Handles phone-based communications",
                  "requiredTools": ["phone_tool"],
                  "executionStrategy": "direct"
                }
              ]
            }
          ],
          "llm_config": {
            "provider": "anthropic",
            "model": "claude-3-haiku-20240307",
            "temperature": 0.3,
            "maxTokens": 2000
