              "description": "Integrates with multiple external databases",
              "preferred_llm": {
                "provider": "mistral",
                "model": "mistral-large-latest",
                "reason": "Best for data analysis and pattern recognition"
              },
              "capabilities": [
                {
                  "id": "multi_system_search",
                  "name": "Multi-System API Search",
                  "description": "Searches across court, AML, fraud, and criminal databases",
                  "requiredTools": ["court_api", "aml_api", "fraud_api", "criminal_api"],
                  "executionStrategy": "direct"
                }
              ]
            },
            {
              "name": "compliance_checking",
              "level": 5,
              "description": "Regulatory compliance and risk assessment",
              "preferred_llm": {
                "provider": "anthropic",
                "model": "claude-3-opus-20240229",
                "reason": "Excellent for regulatory compliance analysis"
              }
            },
            {
              "name": "risk_assessment",
              "level": 5,
              "description": "Evaluates customer risk profile",
              "preferred_llm": {
                "provider": "mistral",
                "model": "mistral-large-latest",
                "reason": "Best for risk analysis and decision making"
              }
            }
          ],
          "llm_config": {
            "provider": "mistral",
            "model": "mistral-large-latest",
            "temperature": 0.1,
            "maxTokens": 4000
          },
          "llm_overrides": {
            "data_analysis": {
              "provider": "mistral",
              "model": "mistral-large-latest"
            },
            "compliance_analysis": {
              "provider": "anthropic",
              "model": "claude-3-opus-20240229"
            }
          },
          "workflows": ["background_verification"],
          "system_prompt": {
            "role": "Background Verification Specialist",
            "goal": "Comprehensive background and credit verification",
            "instructions": "Search all relevant databases, analyze results, make risk-based decisions, ensure compliance"
          }
        }
      },
      
      "account_opening_agent": {
        "name": "Account Opening Agent",
        "type": "automation",
        "description": "Automated account setup and documentation",
        "config": {
          "personality": {
            "friendliness": 0.8,
            "formality": 0.8,
            "proactiveness": 0.9,
            "detail_orientation": 0.9
          },
          "skills": [
            {
              "name": "account_setup",
              "level": 5,
              "description": "Complete account opening procedures",
              "preferred_llm": {
                "provider": "openai",
                "model": "gpt-4-turbo",
                "reason": "Best for complex procedural tasks"
              },
              "capabilities": [
                {
                  "id": "document_generation",
                  "name": "Document Generation",
                  "description": "Creates account opening documents and instructions",
                  "requiredTools": ["document_tool", "template_engine"],
