{
  "customer_onboarding_system": {
    "description": "Complete customer onboarding system matching the sophisticated financial institution workflow",
    "agents": {
      "ai_ocr_agent": {
        "name": "AI OCR Agent",
        "type": "document_processing",
        "description": "Ensemble OCR processing with multiple providers",
        "config": {
          "personality": {
            "friendliness": 0.7,
            "formality": 0.9,
            "proactiveness": 0.8,
            "detail_orientation": 1.0
          },
          "skills": [
            {
              "name": "document_processing",
              "level": 5,
              "description": "Advanced document analysis and processing",
              "preferred_llm": {
                "provider": "mistral",
                "model": "mistral-large-latest",
                "reason": "Best for document analysis and validation"
              },
              "capabilities": [
                {
                  "id": "ensemble_ocr",
                  "name": "Ensemble OCR Processing",
                  "description": "Uses multiple OCR engines for maximum accuracy",
                  "requiredTools": ["tesseract_tool", "azure_ocr", "google_vision"],
                  "executionStrategy": "hybrid",
                  "metadata": {
                    "engines": ["tesseract", "azure", "google_vision"],
                    "confidence_threshold": 0.85,
                    "ensemble_voting": true
                  }
                }
              ]
            },
            {
              "name": "document_validation",
              "level": 5,
              "description": "Validates document completeness and accuracy",
              "preferred_llm": {
                "provider": "anthropic",
                "model": "claude-3-opus-20240229",
                "reason": "Excellent for document validation and compliance"
              }
            }
          ],
          "llm_config": {
            "provider": "mistral",
            "model": "mistral-large-latest",
            "temperature": 0.1,
            "maxTokens": 4000
          },
          "llm_overrides": {
            "document_analysis": {
              "provider": "mistral",
              "model": "mistral-large-latest"
            },
            "visual_processing": {
              "provider": "google",
              "model": "gemini-1.5-pro"
            },
            "compliance_checking": {
              "provider": "anthropic",
              "model": "claude-3-opus-20240229"
            }
          },
          "fallback_llm": {
            "provider": "openai",
            "model": "gpt-4-turbo"
          },
          "workflows": ["customer_onboarding_ocr"],
          "system_prompt": {
            "role": "AI OCR Specialist",
            "goal": "Process customer applications with maximum accuracy using ensemble OCR",
            "instructions": "Use multiple OCR engines, validate results, ensure document completeness, flag any issues for human review"
          }
        }
      },
      
      "background_check_agent": {
        "name": "Background Check Agent",
        "type": "verification",
        "description": "Comprehensive background and credit verification",
        "config": {
          "personality": {
            "friendliness": 0.5,
            "formality": 1.0,
            "proactiveness": 0.9,
            "detail_orientation": 1.0
          },
          "skills": [
            {
              "name": "data_integration",
