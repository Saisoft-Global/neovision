          "system_prompt": {
            "role": "Customer Communication Specialist",
            "goal": "Provide clear, timely communication to customers",
            "instructions": "Send appropriate notifications based on workflow status, use professional tone, ensure clarity"
          }
        }
      }
    },
    
    "workflows": {
      "complete_onboarding_flow": {
        "description": "End-to-end customer onboarding process",
        "stages": [
          {
            "stage": "Document Processing",
            "agent": "ai_ocr_agent",
            "workflow": "customer_onboarding_ocr",
            "description": "OCR processing and document validation"
          },
          {
            "stage": "Background Verification",
            "agent": "background_check_agent", 
            "workflow": "background_verification",
            "description": "Comprehensive background and credit checks"
          },
          {
            "stage": "Account Opening",
            "agent": "account_opening_agent",
            "workflow": "account_opening_automation",
            "description": "Automated account setup and documentation"
          },
          {
            "stage": "Customer Communication",
            "agent": "notification_agent",
            "description": "Multi-channel customer notifications"
          }
        ]
      }
    },
    
    "integration_points": {
      "external_apis": [
        {
          "name": "Court Order Database",
          "type": "court_system",
          "endpoint": "https://api.courtsystem.gov",
          "capabilities": ["mareva_injunction_search", "court_order_lookup"]
        },
        {
          "name": "AML Database",
          "type": "aml_system", 
          "endpoint": "https://api.aml-provider.com",
          "capabilities": ["aml_screening", "sanctions_check"]
        },
        {
          "name": "Fraud Detection System",
          "type": "fraud_system",
          "endpoint": "https://api.fraud-detection.com",
          "capabilities": ["fraud_risk_assessment", "identity_verification"]
        },
        {
          "name": "Criminal Records Database",
          "type": "criminal_db",
          "endpoint": "https://api.criminal-db.gov",
          "capabilities": ["criminal_background_check", "record_retrieval"]
        }
      ],
      "notification_channels": [
        {
          "type": "email",
          "provider": "smtp",
          "templates": ["missing_document", "account_flagged", "account_approved"]
        },
        {
          "type": "phone",
          "provider": "twilio",
          "capabilities": ["voice_call", "sms"]
        },
        {
          "type": "mail",
          "provider": "postal_service",
          "capabilities": ["physical_mail_delivery"]
        }
      ]
    },
    
    "success_metrics": {
      "processing_time": {
        "ocr_processing": "< 5 minutes",
        "background_check": "< 24 hours",
        "account_opening": "< 2 hours",
        "total_process": "< 48 hours"
      },
      "accuracy": {
        "ocr_accuracy": "> 99%",
        "background_check_accuracy": "> 99.5%",
        "false_positive_rate": "< 0.1%"
      },
      "compliance": {
        "regulatory_compliance": "100%",
        "audit_trail": "Complete",
        "data_security": "Enterprise-grade"
      }
    }
  }
